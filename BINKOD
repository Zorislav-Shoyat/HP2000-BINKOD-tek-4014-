10  REM PRINT OUT BINARNI KOD ON TEKTRONIX
20  GOSUB 4000
30  GOSUB 4100
40  REM S1 IS ANIMATION STEP
50  S1=21
60  X9=0
70  Y9=48
80  GOSUB 4200
90  ENTER 2,E0,E$
100  DIM X[512],Y[512],A[512],B[512]
110  I=1
120  RESTORE 1000
130  READ X1,Y1
140  IF X1=0 AND Y1=0 THEN 190
150  X[I]=X1
160  Y[I]=Y1
170  I=I+1
180  GOTO 130
190  RESTORE 2000
200  I=1
210  READ X1,Y1
220  IF X1=0 AND Y1=0 THEN 270
230  A[I]=(X1-X[I])/S1
240  B[I]=(Y1-Y[I])/S1
250  I=I+1
260  GOTO 210
270  X[I]=0
280  Y[I]=0
290  N=I-1
300  FOR S=0 TO S1
310  GOSUB 4100
320  I=N
330  IF I=0 THEN 510
340  X9=X[I]
350  Y9=Y[I]
360  X9=X9/29*1024
370  Y9=768-Y9/29*1024
380  X[I]=X[I]+A[I]
390  Y[I]=Y[I]+B[I]
400  I=I-1
410  GOSUB 4200
420  X9=X[I]
430  Y9=Y[I]
440  X9=X9/29*1024
450  Y9=768-Y9/29*1024
460  X[I]=X[I]+A[I]
470  Y[I]=Y[I]+B[I]
480  I=I-1
490  GOSUB 4300
500  GOTO 330
510  ENTER 1,E0,E$
520  NEXT S
530  RESTORE 2000
540  FOR I=1 TO .97 STEP -.003
550  GOSUB 700
560  RESTORE 2000
570  NEXT I
575  ENTER 3,E0,E$
580  REM OD MATEMATIKE DO UMJETNOSTI
585  GOSUB 4100
590  RESTORE 3000
600  GOSUB 700
610  GOTO 980
700  READ X9,Y9
710  IF X9=0 AND Y9=0 THEN 850
720  X9=X9/29*1024
730  X9=X9*I
740  Y9=768-Y9/29*1024
750  Y9=Y9*I
760  GOSUB 4200
770  READ X9,Y9
780  X9=X9/29*1024
790  X9=X9*I
800  Y9=768-Y9/29*1024
810  Y9=Y9*I
820  GOSUB 4300
840  GOTO 700
850  RETURN 
980  REM MOVE "DONE"
982  X9=0
984  Y9=48
986  GOSUB 4200
999  END 
