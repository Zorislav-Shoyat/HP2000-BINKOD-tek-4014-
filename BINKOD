4000  REM TEKTRONIX OUTPUT ROUTINES
4010  REM ========================
4020  REM
4030  REM -----------------------
4040  REM    INITIALISE
4050  REM -----------------------
4060  GOTO 5120
4070  REM
4100  REM -----------------------
4110  REM   ERASE SCREEN
4120  REM -----------------------
4130  GOTO 5290
4140  REM
4200  REM -----------------------
4210  REM  PEN UP TO X9, Y9
4220  REM -----------------------
4222  X8=X9
4224  Y8=Y9
4230  GOTO 5350
4240  REM
4250  REM -----------------------
4252  REM PEN UP RELATIVE BY X9, Y9
4254  REM ------------------------
4260  X9=X9+X8
4270  Y9=Y9+Y8
4280  GOTO 4200
4300  REM -----------------------
4310  REM  PEN DOWN TO X9, Y9
4320  REM -----------------------
4322  X8=X9
4324  Y8=Y9
4330  GOTO 5200
4340  REM
4350  REM ------------------------
4352  REM PEN DOWN RELATIVE BY X9, Y9
4354  REM ------------------------
4360  X9=X9+X8
4370  Y9=Y9+Y8
4380  GOTO 4300
4400  REM -----------------------
4410  REM    ALPHA MODE
4420  REM -----------------------
4430  GOTO 5460
4440  REM
4500  REM ------------------------
4510  REM  READ JOYSTICK TO X9, Y9
4520  REM ------------------------
4530  GOTO 5530
4540  REM
4600  REM ------------------------
4610  REM   CREATE PICTURE
4620  REM ------------------------
4630  GOTO 5780
4640  REM
4700  REM ========================
5000  REM ********************************************
5010  REM * STANFORD GRADUATE SCHOOL OF BUSINESS     *
5020  REM * PROGRAM NAME: GTEK                       *
5030  REM * SUBROUTINES FOR USING THE TEKTRONIX      *
5040  REM * T4002 TERMINAL WITH THE HEWLETT-PACKARD  *
5050  REM * 2000C.                                   *
5060  REM * PROGRAMMED BY W.F. SHARPE (APRIL, 1971)  *
5070  REM * NOTE: A SPECIAL FILE NAMED $STRING MUST  *
5080  REM * BE AVAILABLE (SEE DOCUMENTATION).        *
5090  REM ********************************************
5092  REM
5093  REM * ENHANCED BY ZORISLAV SHOYAT *
5094  REM * (DECEMBER, 2021)            *
5097  REM
5100  REM -------------------------
5110  REM -- get strings
5120  FILES $VCHAR
5130  DIM H$[32],X$[64],Y$[33]
5140  READ #1;X$,Y$
5150  H$[1,32]=X$[33,64]
5160  Z$[1,1]=Y$[33]
5170  X$=X$[1,32]
5171  Y$=Y$[1,32]
5180  RETURN 
5190  REM -------------------------
5195  REM - CLIPPING (ZS)
5200  X9=INT(X9+.5)
5202  IF X9 >= 0 THEN 5210
5204  X9=0
5206  GOTO 5220
5210  IF X9<1024 THEN 5220
5212  X9=1023
5220  Y9=INT(Y9+.5)
5222  IF Y9 >= 0 THEN 5230
5224  Y9=0
5226  GOTO 5240
5230  IF Y9<1024 THEN 5240
5232  Y9=1023
5240  PRINT  USING "#,A";H$[INT(Y9/32)+1,INT(Y9/32)+1]
5245  PRINT  USING "#,A";Y$[Y9-(32*INT(Y9/32))+1,Y9-(32*INT(Y9/32))+1]
5250  PRINT  USING "#,A";H$[INT(X9/32)+1,INT(X9/32)+1]
5255  PRINT  USING "#,A";X$[X9-(32*INT(X9/32))+1,X9-(32*INT(X9/32))+1]
5260  RETURN 
5270  REM --------------------------
5280  REM -- erase screen
5290  REM PRINT ESC, CTRL-L
5291  PRINT '24
5300  PRINT Z$;'12;
5310  RETURN 
5320  REM --------------------------------
5330  REM -- move to x9,y9 with pen up
5340  REM -- 1310 PRINT "gs";
5350  PRINT  USING 5360
5360  IMAGE #,""
5370  GOSUB 5190
5380  RETURN 
5390  REM -----------------------------
5400  REM -- move to x9,y9 with pen down
5410  GOSUB 5190
5420  RETURN 
5430  REM --------------------------------
5440  REM -- put in alpha mode
5450  REM -- 1510 PRINT "us";
5460  PRINT  USING 5470
5470  IMAGE #,""
5480  RETURN 
5490  REM -----------------------------------
5500  REM -- read joystick
5510  REM -- uses z8,z9,c$
5520  REM -- PRINT ESC, CTRL-Z
5530  PRINT  USING 5540;Z$
5540  IMAGE #,A,"ï¿½"
5550  DIM C$[5]
5560  Z8=250
5570  ENTER Z8,Z9,C$
5571  IF Z9<0 THEN 5570
5580  GOSUB 5650
5590  X9=Z9
5595  REM ?? PRINT C$
5600  C$[2,2]=C$[4,4]
5610  C$[3,3]=C$[5,5]
5620  GOSUB 5650
5630  Y9=Z9
5640  RETURN 
5650  FOR Z8=1 TO 32
5660  IF H$[Z8,Z8]=C$[2,2] THEN 5690
5670  NEXT Z8
5680  Z8=32
5690  Z9=32*(Z8-1)
5700  FOR Z8=1 TO 32
5710  IF H$[Z8,Z8]=C$[3,3] THEN 5740
5720  NEXT Z8
5730  Z8=32
5740  Z9=Z9+(Z8-1)
5750  RETURN 
5760  REM -----------------------------
5770  REM -- subroutine to create picture
5780  DIM I$[72]
5790  READ I$
5800  IF I$="e" THEN 5980
5810  IF I$="E" THEN 5980
5820  IF I$="a" THEN 5930
5830  IF I$="A" THEN 5930
5840  READ X9,Y9
5850  IF I$="d" THEN 5900
5860  IF I$="D" THEN 5900
5870  REM -- move to x9,y9 with pen up
5880  GOSUB 5320
5890  GOTO 5790
5900  REM -- move to x9,y9 with pen down
5910  GOSUB 5390
5920  GOTO 5790
5930  REM -- shift to apha and print next string
5940  GOSUB 5430
5950  READ I$
5960  PRINT I$
5970  GOTO 5790
5980  REM -- end reached
5990  RETURN 
6000  END 
9000  GOSUB 4000
9010  GOSUB 4100
9999  END 
